### Run settings ###
timestep	0.005
neighbor	0.8 bin # 0.032
neigh_modify	delay 1 every 1 check yes

# Grafting layer
group	grafting type 1
group	villi type 2
group	mobile subtract all grafting
group	bacteria subtract mobile villi
fix	zwalls all wall/reflect zlo EDGE zhi EDGE # for grafting beads 100 1 1 ### OR 100 (MAIN) OR 10 OR 1?
fix	freezeGrafting grafting setforce 0 0 0
neigh_modify	exclude group grafting grafting
neigh_modify	one ${neigh_modify_size} page ${page_size}

### Output ###
dump	2 all atom 1000 dump.lammpstrj

# Initial velocity

compute	Tmobile mobile temp
compute_modify	Tmobile dynamic/dof yes ## ???

compute	Tbacteria bacteria temp
compute_modify	Tbacteria dynamic/dof yes ## ???
compute	Tvilli villi temp
compute_modify	Tvilli dynamic/dof yes

compute	KEbacteria bacteria ke
compute	KEvilli villi ke

compute	PEbacteriaatom bacteria pe/atom pair
compute	PEbacteria bacteria reduce sum c_PEbacteriaatom

compute	PEvilliatom villi pe/atom pair bond angle
compute	PEvilli villi reduce sum c_PEvilliatom

#compute pairs bacteria property/local patom1 patom2
#compute interactions bacteria pair/local dist eng force
#dump 3 bacteria local 50000 pairwise.dump index c_pairs[1] c_pairs[2] c_interactions[1] c_interactions[2] c_interactions[3]

velocity	villi create 0.85 5324324 temp Tvilli
velocity	bacteria create ${bac_veloc} 5324324 temp Tbacteria
thermo	1000
thermo_style	custom step temp pe ke etotal press c_Tbacteria c_Tvilli c_KEbacteria c_KEvilli c_PEbacteria c_PEvilli  cpuremain
thermo_modify	lost ignore ## ???

# Minimize
minimize	1E-8 1E-8 10000 10000

# Run with limit and frequent balance
fix	nveinit_villi	villi nve/limit 1
fix	nveinit_bacteria	bacteria nve/limit ${bac_nve_limit}
fix	lv_villi	villi langevin 0.85 0.85 10 334232 # 1000 or 100000
fix	lv_bacteria	bacteria langevin ${temp_bac} ${temp_bac} 100 334232 # 1000 or 100000
fix_modify	lv_villi	temp Tvilli
fix_modify	lv_bacteria	temp Tbacteria
fix	balance	mobile balance 10000 1.1 shift z 10 1.1 weight time 1 # 10000 or 100000

run	10000

minimize	1E-8 1E-8 10000 10000

unfix	nveinit_villi
unfix	nveinit_bacteria
unfix	lv_villi
unfix	lv_bacteria
fix	nve	mobile nve
fix	lv_villi	villi langevin 0.85 0.85 1 334232 # 100 or 10000
fix	lv_bacteria	bacteria langevin ${temp_bac} ${temp_bac} 10 334232 # 100 or 10000
fix_modify	lv_villi	temp Tvilli
fix_modify	lv_bacteria	temp Tbacteria
run	${n_steps}

write_restart	restart.equi
write_data	data.equi
